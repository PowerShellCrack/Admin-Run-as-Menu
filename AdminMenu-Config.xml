<?xml version="1.0"?>
<app>
	<!-- https://github.com/richardtracy79/System-Administrator-Run-as-Tool.git -->
	<title>System Administrator Run-as Tool</title>
	<version>3.4.2</version>
	<configs>
		<debugmode>True</debugmode>
		<useRemote>
			<remote>False</remote>
			<path>\\SERVERNAME\SHARE\Path-to-Config.xml</path>
		</useRemote>

		<functionScripts>
			<path>Extensions\AdminMenu-Extensions.ps1</path>
			<path>Extensions\AdminMenu-Extensions2.ps1</path>
		</functionScripts>
		<hideIfNotUsed>False</hideIfNotUsed> <!--This will hide the app when not the main window, the app can be opened from the task tray -->
		<checkRSAT>True</checkRSAT> <!--This can take a little longer to load tool -->
	</configs>
	
	<!--
	Special Characters to use:
	[&#xD;&#xA;] -> use in between space in name to place the next characters on the next line 
	[&amp;] -> use inplace of '&' symbol
	
	Settings:
	-You can disable a button by adding enable="false" like:
		<button id="01" Name="Powershell" bgcolor="#FF0066CC" textcolor="White" enable="false"/>
	-You can hide any tab (except the first one) by adding enable="false" like:
		<tab id="2" name="Ent" extension="Extensions\AdminMenu-Tab2.ps1" enable="false">
		NOTE: Any tabs configured without a corresponding ps1 file will automatically be hidden
	
	
	-->
	<tabAndButtons>
		<hideUnusedButtons>True</hideUnusedButtons>
		
		<tab id="1" name="Sys" extension="Extensions\AdminMenu-Tab1.ps1">
			<section1Label>System Tools</section1Label>
			<section2Label>Remote Tools</section2Label>
			<button id="01" Name="Powershell" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="02" Name="Command&#xD;&#xA;Prompt" bgcolor="Black" textcolor="White"/>
			<button id="03" Name="CMtrace" bgcolor="#FFFFFFFF" textcolor="Black"/>
			<button id="04" Name="Microsoft&#xD;&#xA;Management&#xD;&#xA;Console" bgcolor="#FFD40000" textcolor="White"/>
			<button id="05" Name="Dameware&#xD;&#xA;Remote&#xD;&#xA;Utilities" bgcolor="#FF81D681" textcolor="Black"/>
			<button id="07" Name="Registry" bgcolor="#FF117E00" textcolor="White"/>
			<button id="11" Name="Remote&#xD;&#xA;Desktop&#xD;&#xA;Connection" bgcolor="#FF99B4D1" textcolor="Black"/>
			<button id="12" Name="Dameware&#xD;&#xA;Mini&#xD;&#xA;Remote&#xD;&#xA;Control" bgcolor="#FF81D681" textcolor="Black"/>
			<button id="13" Name="Computer&#xD;&#xA;Management" bgcolor="#FFE3E3E3" textcolor="Black"/>
			<button id="15" Name="Ping" bgcolor="#FF000000" textcolor="White"/>
			<button id="16" Name="System&#xD;&#xA;Info&#xA;(CMD)" bgcolor="#FF000000" textcolor="White"/>
			<button id="17" Name="System&#xD;&#xA;Info&#xA;(PS1)" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="18" Name="Services" bgcolor="#FF6D6D6D" textcolor="White"/>
			<button id="21" Name="Current&#xD;&#xA;User" bgcolor="#FFFBFAFA" textcolor="Black"/>
			<button id="22" Name="Event&#xD;&#xA;Viewer" bgcolor="#FFBDA27E" textcolor="Black"/>
			<button id="26" Name="Task&#xD;&#xA;Manager" bgcolor="#FFFBFAFA" textcolor="Black"/>
		</tab>
		<tab id="2" name="Ent" extension="Extensions\AdminMenu-Tab2.ps1">
			<section1Label>AD Consoles</section1Label>
			<section2Label>Tools &amp; Scripts</section2Label>
			<button id="01" Name="Active&#xD;&#xA;Directory&#xD;&#xA;Users and&#xD;&#xA;Computers" bgcolor="#FFB9B553" textcolor="Black"/>
			<button id="02" Name="Active&#xD;&#xA;Directory&#xD;&#xA;Sites and&#xD;&#xA;Services" bgcolor="#FF3399FF" textcolor="White"/>
			<button id="03" Name="Group Policy&#xD;&#xA;Management&#xD;&#xA;Console" bgcolor="#FFFDFAB5" textcolor="Black"/>
			<button id="07" Name="AD&#xD;&#xA;Admin&#xD;&#xA;Center" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="08" Name="DNS&#xA;Console" bgcolor="#FFB5D9FD" textcolor="Black"/>
			<button id="09" Name="DHCP&#xA;Console" bgcolor="#FFECBF28" textcolor="Black"/>
			<button id="11" Name="AD&#xD;&#xA;Powershell&#xD;&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="18" Name="Domain&#xD;&#xA;RDP" bgcolor="#FFB4B4B4" textcolor="Black"/>
		</tab>
		<tab id="3" name="Auto" extension="Extensions\AdminMenu-Tab3.ps1">
			<section1Label>SCCM Consoles</section1Label>
			<section2Label>Tools &amp; Scripts</section2Label>
			<button id="01" Name="SCCM&#xD;&#xA;Console" bgcolor="#FF99B4D1" textcolor="Black"/>
			<button id="02" Name="MDT&#xD;&#xA;Deployment&#xD;&#xA;Workbench" bgcolor="#FFB83B3B" textcolor="White"/>
			<button id="07" Name="Client&#xD;&#xA;Spy" bgcolor="#FF6E3BB8" textcolor="White"/>
			<button id="11" Name="SCCM&#xA;Powershell&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="18" Name="Remote&#xD;&#xA;into&#xA;CAS" bgcolor="#FFB4B4B4" textcolor="Black"/>
			<button id="22" Name="Remote&#xA;into&#xA;Primary" bgcolor="#FFB4B4B4" textcolor="Black"/>
		</tab>
		<tab id="4" name="Exch" extension="Extensions\AdminMenu-Tab4.ps1">
			<section1Label>UC Consoles</section1Label>
			<section2Label>Tools &amp; Scripts</section2Label>
			<button id="01" Name="Exchange&#xA;Console" bgcolor="#FF127A2A" textcolor="White"/>
			<button id="02" Name="Skype&#xD;&#xA;Console" bgcolor="#FFB83B3B" textcolor="White"/>
			<button id="11" Name="Exchange&#xA;Powershell&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="17" Name="Skype&#xA;Powershell&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="18" Name="Remote&#xA;into&#xA;Exchange" bgcolor="#FFB4B4B4" textcolor="Black"/>
		</tab>
		<tab id="5" name="Infra" extension="Extensions\AdminMenu-Tab5.ps1">
			<section1Label>Infrastructure Consoles</section1Label>
			<section2Label>Tools &amp; Scripts</section2Label>
			<button id="01" Name="Vmware&#xD;&#xA;Vsphere" bgcolor="#FF256631" textcolor="White"/>
			<button id="02" Name="VMM&#xD;&#xA;Console" bgcolor="#FF3399FF" textcolor="White"/>
			<button id="07" Name="Hyper-V" bgcolor="#FF06007E" textcolor="White"/>
			<button id="11" Name="VMware&#xA;Powershell&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="12" Name="UCS&#xA;Powershell&#xA;Module" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="18" Name="Putty" bgcolor="#FF000000" textcolor="Black"/>
		</tab>
		
		<!-- 
			There are only 24 buttons available for tab 6
				Place additional powershell scripts in the extra folder
		-->
		<tab id="6" name="Extra" extension="Extensions\AdminMenu-Tab6.ps1">
			<section1Label>Extra Tools &amp; Scripts</section1Label>
			<button id="01" Name="Clipboard&#xA;History" bgcolor="#FF0066CC" textcolor="White"/>
			<button id="02" Name="Patch&#xA;and&#xA;Audit&#xA;Utility" bgcolor="#FF0066CC" textcolor="White"/>
		</tab>
		
	</tabAndButtons>
	
	<supportedOSbuilds>
		<OS>
			<name>Microsoft Windows 10 Enterprise</name>
			<version>10.0</version>
			<arc>x64</arc>
		</OS>
		<OS>
			<name>Microsoft Windows 8.1 Enterprise</name>
			<version>6.3</version>
			<arc>x64</arc>
		</OS>
		<OS id="minimum">
			<name>Microsoft Windows 7 Enterprise</name>
			<version>6.1</version>
			<arc>x64</arc>
		</OS>
	</supportedOSbuilds>
	
</app>